{
  "name": "blindquest_js",
  "version": "1.0.0",
  "description": "Audio-exclusive Rogue-like written in Js",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && broccoli build dist",
    "build-cmd": "del /F dist && broccoli build dist",
    "build-powershell": "Remove-Item -Recurse dist && broccoli build dist",
    "build-prod": "rm -rf dist_prod && BROCCOLI_ENV=production broccoli build dist_prod",
    "serve": "broccoli serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guydunigo/BlindQuest_Js.git"
  },
  "author": "Guillaume GONI",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guydunigo/BlindQuest_Js/issues"
  },
  "homepage": "https://github.com/guydunigo/BlindQuest_Js#readme",
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "broccoli": "^1.1.3",
    "broccoli-babel-transpiler": "^6.1.2",
    "broccoli-concat": "^3.2.2",
    "broccoli-env": "0.0.1",
    "broccoli-funnel": "^2.0.1",
    "broccoli-livereload": "^1.2.1",
    "broccoli-merge-trees": "^2.0.0",
    "broccoli-rollup": "^1.3.0",
    "howler": "^2.0.4"
  }
}
